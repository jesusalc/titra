    # ---
    try_files $uri/index.html $uri @titra;

    location @titra {
        root /home/zeus/_/work/ubemax/projects/titra/public;
        proxy_pass http://titra;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
